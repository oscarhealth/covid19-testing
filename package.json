  "scripts": {
    "build": "NODE_PATH=$(pwd) NODE_OPTIONS=--max_old_space_size=4096 webpack --progress --config javascript/test-site-finder/webpack.config.babel.js --bail",
    "gen-lang-yaml": "rm -rf .tx/extract && babel javascript/ --quiet --ignore '**/dist/*.js' --out-dir /tmp/.tx && babel-node javascript/build_tools/translate/BuildSourceFile.js",
    "prepare-translations": "npm run gen-lang-yaml | sed '1,5d' > javascript/test-site-finder/static/intl/message_es.yml",
    "webpack-dev": "NODE_PATH=$(pwd) NODE_OPTIONS=--max_old_space_size=4096 webpack-dev-server --progress --debug --env.logger --config=javascript/test-site-finder/webpack.config.babel.js"
  },
